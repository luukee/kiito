<div itemscope itemtype="http://schema.org/Product">

  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
  <meta itemprop="image" content="{{ product.featured_image.src | img_url: 'grande' }}">

  <div class="{{ block.settings.columns }} px-lg-3 px-2 py-5 my-lg-5 shop-products" id="{{ block.settings.productid }}">

      <div class="d-flex flex-column justify-content-between pb-5">

          <!-- header -->
          <div class="header">

              <!-- product image -->
              <div class="image" data-transition="entrance" style="transform : translateY( 50px )">

                  <div class="pb-lg-3 mb-2 mb-lg-3">

                      <div class="embed-responsive product-img embed-responsive-17by20">

                          <div class="embed-responsive-item">

                              <img class="lazyload w-100" data-src="{{ product.images.first.src | img_url : 'master' }}" alt="{{ product.images.first.alt }}" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-expand="-50">

                          </div>

                      </div>

                  </div>

              </div>

              <!-- product title -->
              {% if product.title != blank %}

                  <div class="title" data-transition="entrance" style="transform : translateY( 50px )">

                      <div class="text-center">

                          <h1 class="text-uppercase mb-1">

                              {{ product.title }}

                          </h1>

                      </div>

                  </div>

              {% endif %}

              {% if product.description != blank %}

              <div class="description" data-transition="entrance" style="transform : translateY( 50px )">

                  <div class="text-center py-2">

                      {{ product.description }}

                  </div>

              </div>

              {% endif %}

          </div> <!-- end header -->


          <!-- product info lightbox -->
        {% if block.settings.link != blank %}

          <div class="link mb-2" data-transition="entrance" style="transform : translateY( 50px )">

              <div class="text-center">

                  <a class="product-link" href="{{ block.settings.lightbox | img_url : 'master' }}" data-fancybox>

                      <h6>

                          {{ block.settings.link }}

                      </h6>

                  </a>

              </div>

          </div>

        {% endif %}


          <!-- price info -->
          <div class="price-info pb-1" data-transition="entrance" style="transform : translateY( 50px )">

              <div class="prices">

                  <p class="pricing">

                    {% if block.settings.pricecut != blank %}

                      <span class="price-cut">

                          {{ block.settings.pricecut }}

                      </span>

                    {% endif %}

                    {% if block.settings.priceinfo != blank %}

                      <span>

                          {{ block.settings.priceinfo }}

                      </span>

                    {% endif %}

                  </p>

                  {% if block.settings.priceinfosubtext != blank %}

                      <p class="price-info-subtext">

                          {{ block.settings.priceinfosubtext }}

                      </p>

                  {% endif %}

              </div>

          </div>


          <!-- footer -->
          <div class="footer">

              <div class="button" data-transition="entrance" style="transform : translateY( 50px )">

                  <div class="mb-2">

                      <form action="/cart/add" data-productid="{{product.id}}" method="post" enctype="multipart/form-data">

                          <div class="form-group">

                              <!-- ReCharge Select -->

                              <style>#quantity { padding:5px; width:35px; border: 1px solid #555; } .hide { display:none; }</style>

                              <select name="id" data-productid="{{ product.id }}" class="hide">

                                  {% for variant in product.variants %}

                                      {% if variant.available == true %}

                                          <option value="{{variant.id}}"> {{ variant.title }} for {{ variant.price | money_with_currency }}</option>

                                      {% else %}

                                          <option disabled="disabled"> {{ variant.title }} - sold out!</option>

                                      {% endif %}

                                  {% endfor %}

                              </select>

                              <!-- ReCharge subscription product -->
                              {% if product.available %}

                                  {% include 'subscription-product' %}

                              {% endif %}

                              <!-- add to cart button -->

                              <button data-productid="{{product.id}}" class="btn btn--to-secondary btn--full product__add-to-cart-button ajax_form_cart single_add_to_cart_button"
                                type="submit"
                                id="add"
                                value="Add to Cart"
                                aria-label="{{ 'products.product.add_to_cart' | t }}"
                              >
                                <span class="secondary-text" aria-hidden=true data-cart-secondary-submit-text>{{ 'products.product.add_to_cart' | t }}</span>
                              </button>


                      </div> <!-- END form-group -->

                  </form>

