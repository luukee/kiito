              <a class="text-white" href="{{ link.url }}" {% if link.url contains 'http' %}target="_blank"{% endif %}>

                {{ link.title }}

              </a>

              <br>

            {% endfor %}

            {% comment %} wholesale popup contact form {% endcomment %}
            <a data-fancybox data-src="#wholesale" class=" " href="javascript:;">
                Wholesale
            </a>
            {%- comment -%}{%- endcomment -%}
            <div id="wholesale" style="display: none;" class="review wholesale-fancybox">

              <h1 class="text-uppercase text-center">Wholesale Request</h1>

              <p>Please submit the form below to request access to our wholesale products.</p>
              
              <div class="wholesale-contact-form">
                
                {% include 'wholesale-contact-form' %}
                
              </div>

            </div>
            
            {% comment %}END wholesale popup contact form{% endcomment %}
          </p>

          <p class="mb-5">

            <u>
              
              {% comment %}

              <a href="mailto:{{ shop.email }}">

                {{ shop.email }}

              </a>

              <br>
              
              {% endcomment %}

              <a href="mailto:hi@drinkkiito.com">hi@drinkkiito.com</a>

            </u>

          </p>

