{% schema %}

  {

    "settings" : [

      {

        "default" : "Title",

        "label" : "Title",

        "type" : "text",

        "id" : "title"

      },

      {

        "default" : "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur consectetur sapien in quam auctor.",

        "type" : "textarea",

        "label" : "Instagram HTML Snippet",

        "id" : "text_top"

      },

      {

        "info" : "You can find this in MailChimp.",

        "label" : "Action",

        "id" : "action",

        "type" : "text"

      },

      {

        "info" : "Leave blank to show no message",

        "default" : "We value your privacy",

        "label" : "Message",

        "id" : "message",

        "type" : "text"

      },

      {

        "default" : "Thank you",

        "label" : "Success",

        "id" : "success",

        "type" : "text"

      },

      {

        "default" : "Please try again",

        "label" : "Error",

        "type" : "text",

        "id" : "error"

      },

      {

        "default" : "Enter your email address",

        "label" : "Placeholder",

        "id" : "placeholder",

        "type" : "text"

      },

      {

        "default" : "Subscribe",

        "label" : "Button",

        "type" : "text",

        "id" : "button"

      }

    ],

    "blocks" : [

      {

        "name" : "Image",

        "type" : "image",

        "settings" : [

          {

            "type" : "image_picker",

            "label" : "Image",

            "info" : "1×1",

            "id" : "image"

          },

          {

            "default" : "/",

            "label" : "URL",

            "type" : "url",

            "id" : "url"

          }

        ]

      }

    ]

  }

{% endschema %}
